mod example {
    // Primitive types:
    // `i8`, `i16`, `i32`, `i64`, `u8`, `u16`, `u32`, `u64`, `f32`, `f64`.

    // Enumerations

    enum color {
        red,
        blue,
        green,
    }

    // Aliases

    type colour = color;
    type col = colour;

    // Structures

    struct st {
        a: i8,
        b: i32,
        c: color,
    }

    // functions

    fn set_color(to: color) -> bool;

    fn get_structure(of: color) -> st;
}
